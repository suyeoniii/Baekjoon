#include <iostream>
using namespace std;

int main(){
    int y1,m1,d1,y2,m2,d2;
    cin>>y1>>m1>>d1>>y2>>m2>>d2;

    int y,m,d;
    d = d2-d1;
    m = m2-m1;
    y = y2-y1;

    if(d<0){
        d = 31-d;
        m -= 1;
    }
    if(d>31){
        d = d-31;
        m += 1;
    }
    if(m<0){
        m = 12-m;
        y -= 1;
    }
    if(m>12){
        m = m-12;
        y+=1;
    }

    int ans = 0;
    cout<<y<<m<<d;
}
